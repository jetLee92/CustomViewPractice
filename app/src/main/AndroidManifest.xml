		<activity android:name=".ui.property.PropertyMainActivity"/>
		<activity android:name=".ui.property.TypeEvaluatorActivity"/>
		<activity android:name=".ui.CanvasMatrixCamera.ClipActivity"/>
        <activity android:name=".ui.measurelayout.MeasureLayoutActivity" />
		<activity android:name=".ui.measurelayout.FlowTagLayoutActivity" />
		<activity android:name=".ui.measurelayout.CircleActivity" />
		<activity android:name=".ui.touch.Touch1Activity" />
		<activity android:name=".ui.touch.TouchMainActivity" />
		<activity android:name=".ui.touch.PageViewActivity" />
